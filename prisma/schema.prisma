datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

generator client {
  provider = "prisma-client-js"
}

model poetryMaker {
  id          Int       @id @default(autoincrement())
  poem        String
  date        DateTime
  prompt  String
}